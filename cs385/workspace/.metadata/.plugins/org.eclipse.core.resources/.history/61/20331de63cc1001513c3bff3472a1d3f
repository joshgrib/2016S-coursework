#include <iostream>
#include <sstream>
using namespace std;

int main(int argc, char *argv[]){
	istringstream iss(argv[1]);
	istringstream iss2(argv[2]);
	cout << iss << "," << iss2 << endl;
	cout << "Hello" << endl;
	return 0;
}
