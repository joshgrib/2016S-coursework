#include <iostream>
#include <sstream>
using namespace std;

int gcd_rec(int a, int b){
	if((a<0) || (b<0)){
		cerr << "Both numbers must be positive" << endl;
		return -1;
	}
	if( (a==0) || (b==0)){
		return 0;
	}
	if(a == b){
		return a;
	return gcd_red(b, a%b);
}
/*
