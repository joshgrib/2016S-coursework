#include <iostream>
#include <sstream>
using namespace std;

int gcd_rec(int a, int b){
	if((a<0) || (b<0)){
		cerr << "Both number must be positive" << endl;
		return -1;
	}
	if( (a==0) || (b==0)){
		return 0;
	}
	if(b == 0){
		return a;
	}
}

int main(int argc, char *argv[]){
	istringstream iss(argv[1]);
	istringstream iss2(argv[2]);
	cout << iss << "," << iss2 << endl;
	cout << "Hello" << endl;
	return 0;
}
